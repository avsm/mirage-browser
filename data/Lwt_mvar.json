          { "name": "Lwt_mvar.take",
            "location": 
              { "implementation": "unknown",
                "interface": "/Users/avsm/src/git/avsm/mirage/lib/_build/unix-socket/std/lwt_mvar.mli|2732" },
            "info": 
              { "description": "<span class=\"code\" >take mvar</span> will take any currently available value from the mailbox variable. If no value is currently available, the current thread will block, awaiting a value to be <span class=\"code\" >put</span> by another thread." },
            "type": "'a Lwt_mvar.t -> 'a Lwt.t",
