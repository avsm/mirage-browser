      { "value": 
          { "name": "Lwt_mvar.put",
            "location": 
              { "implementation": "unknown",
                "interface": "/Users/avsm/src/git/avsm/mirage/lib/_build/unix-socket/std/lwt_mvar.mli|2459" },
            "info": 
              { "description": "<span class=\"code\" >put mvar value</span> puts a value into a mailbox variable. This value will remain in the mailbox until <span class=\"code\" >take</span> is called to remove it. If the mailbox is not empty, the current thread will block until it is emptied." },
            "type": "'a Lwt_mvar.t -> 'a -> unit Lwt.t",
            "params": [ 
              { "name": "",
                "type": "'a Lwt_mvar.t" }, 
              { "name": "",
                "type": "'a" } ] } }, 
